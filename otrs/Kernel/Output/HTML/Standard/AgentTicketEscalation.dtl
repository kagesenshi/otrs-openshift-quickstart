<!-- dtl:block:TicketEscalationFirstResponseTimeOver -->
<a href="$Env{"Baselink"}Action=AgentTicketZoom;TicketID=$LQData{"TicketID"}" title="$QData{"Title"}">
    $Text{"Ticket %s: first response time is over (%s)!", "$QData{"TicketNumber"}", "$QData{"FirstResponseTimeHuman"} / $TimeShort{"$QData{"FirstResponseTimeDestinationDate"}"}"}
</a>
<!-- dtl:block:TicketEscalationFirstResponseTimeOver -->
<!-- dtl:block:TicketEscalationFirstResponseTimeWillBeOver -->
<a href="$Env{"Baselink"}Action=AgentTicketZoom;TicketID=$LQData{"TicketID"}" title="$QData{"Title"}">
